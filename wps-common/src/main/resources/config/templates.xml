<ac:templates xmlns:ac="http://aodn.org.au/aggregator/configuration">
    <ac:template match=".*bathy_ppb_deakin.*" name="bathymetry">
        <ac:attributes>
            <ac:attribute>
                <ac:name>geospatial_lat_min</ac:name><ac:type>String</ac:type><ac:value>${LAT_MIN}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>geospatial_lat_max</ac:name><ac:type>String</ac:type><ac:value>${LAT_MAX}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>geospatial_lon_min</ac:name><ac:type>String</ac:type><ac:value>${LON_MIN}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>geospatial_lon_max</ac:name><ac:type>String</ac:type><ac:value>${LON_MAX}</ac:value>
            </ac:attribute>
        </ac:attributes>
    </ac:template>
    <ac:template match=".*csiro_cars.*" name="csiro_cars">
        <ac:attributes>
            <ac:attribute>
                <ac:name>geospatial_lat_min</ac:name><ac:type>String</ac:type><ac:value>${LAT_MIN}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>geospatial_lat_max</ac:name><ac:type>String</ac:type><ac:value>${LAT_MAX}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>geospatial_lon_min</ac:name><ac:type>String</ac:type><ac:value>${LON_MIN}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>geospatial_lon_max</ac:name><ac:type>String</ac:type><ac:value>${LON_MAX}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>geospatial_vertical_max</ac:name><ac:type>String</ac:type><ac:value>${Z_MAX}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>geospatial_vertical_min</ac:name><ac:type>String</ac:type><ac:value>${Z_MIN}</ac:value>
            </ac:attribute>
        </ac:attributes>
    </ac:template>
    <ac:template match="srs_ghrsst.*,srs_sst.*" name="srs_ghrsst">
        <ac:attributes>
            <ac:attribute>
                <ac:name>start_time</ac:name><ac:type>String</ac:type><ac:value>${TIME_START}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>stop_time</ac:name><ac:type>String</ac:type><ac:value>${TIME_END}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>time_coverage_start</ac:name><ac:type>String</ac:type><ac:value>${TIME_START}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>time_coverage_end</ac:name><ac:type>String</ac:type><ac:value>${TIME_END}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>title</ac:name><ac:type>String</ac:type><ac:match>.*</ac:match><ac:value>${0}, ${TIME_START}, ${TIME_END}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>southernmost_latitude</ac:name><ac:type>Double</ac:type><ac:value>${LAT_MIN}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>northernmost_latitude</ac:name><ac:type>Double</ac:type><ac:value>${LAT_MAX}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>westernmost_longitude</ac:name><ac:type>Double</ac:type><ac:value>${LON_MIN}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>easternmost_longitude</ac:name><ac:type>Double</ac:type><ac:value>${LON_MAX}</ac:value>
            </ac:attribute>
        </ac:attributes>
        <ac:variables>
            <ac:variable>
                <ac:name>time</ac:name>
            </ac:variable>
            <ac:variable>
                <ac:name>lat</ac:name>
            </ac:variable>
            <ac:variable>
                <ac:name>lon</ac:name>
            </ac:variable>
            <ac:variable>
                <ac:name>dt_analysis</ac:name><ac:type>Float</ac:type>
            </ac:variable>
            <ac:variable>
                <ac:name>l2p_flags</ac:name>
            </ac:variable>
            <ac:variable>
                <ac:name>quality_level</ac:name>
            </ac:variable>
            <ac:variable>
                <ac:name>satellite_zenith_angle</ac:name><ac:type>Float</ac:type>
            </ac:variable>
            <ac:variable>
                <ac:name>sea_surface_temperature</ac:name><ac:type>Float</ac:type>
            </ac:variable>
            <ac:variable>
                <ac:name>sses_bias</ac:name><ac:type>Float</ac:type>
            </ac:variable>
            <ac:variable>
                <ac:name>sses_count</ac:name><ac:type>Float</ac:type>
            </ac:variable>
            <ac:variable>
                <ac:name>sses_standard_deviation</ac:name><ac:type>Float</ac:type>
            </ac:variable>
            <ac:variable>
                <ac:name>sst_dtime</ac:name><ac:type>Float</ac:type>
            </ac:variable>
        </ac:variables>
    </ac:template>
    <ac:template match=".*" name="default">
        <ac:attributes>
            <ac:attribute>
                <ac:name>time_coverage_start</ac:name><ac:type>String</ac:type><ac:value>${TIME_START}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>time_coverage_end</ac:name><ac:type>String</ac:type><ac:value>${TIME_END}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>title</ac:name><ac:type>String</ac:type><ac:match>(.*?)(,[^,]*)?</ac:match><ac:value>${1}, ${TIME_START}, ${TIME_END}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>geospatial_lat_min</ac:name><ac:type>String</ac:type><ac:value>${LAT_MIN}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>geospatial_lat_max</ac:name><ac:type>String</ac:type><ac:value>${LAT_MAX}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>geospatial_lon_min</ac:name><ac:type>String</ac:type><ac:value>${LON_MIN}</ac:value>
            </ac:attribute>
            <ac:attribute>
                <ac:name>geospatial_lon_max</ac:name><ac:type>String</ac:type><ac:value>${LON_MAX}</ac:value>
            </ac:attribute>
        </ac:attributes>
    </ac:template>
</ac:templates>